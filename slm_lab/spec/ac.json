  "ac_cartpole": {
    "agent": [{
      "name": "ActorCritic",
      "algorithm": {
        "name": "ActorCritic",
        "action_pdtype": "default",
        "action_policy": "default",
        "action_policy_update": "no_update",
        "explore_var_start": null,
        "explore_var_end": null,
        "explore_anneal_epi": null,
        "gamma": 0.99,
        "use_gae": false,
        "lam": 1.0,
        "num_step_returns": 100,
        "policy_loss_coef": 1.0,
        "val_loss_coef": 1.0,
        "add_entropy": false,
        "entropy_coef": 0.01,
