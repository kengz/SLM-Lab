{
  "multitask_dqn_boltzmann_cartpole": {
    "agent": [{
      "name": "MultitaskDQN",
      "algorithm": {
        "name": "MultitaskDQN",
        "action_pdtype": "Argmax",
        "action_policy": "multi_boltzmann",
