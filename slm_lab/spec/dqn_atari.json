{
  "dqn_breakout": {
    "agent": [{
      "name": "AtariDQN",
      "algorithm": {
        "name": "DQN",
        "action_policy": "epsilon_greedy",
        "action_policy_update": "linear_decay",
        "explore_var_start": 1.0,
        "explore_var_end": 0.1,
        "explore_anneal_epi": 2500,
        "gamma": 0.99,
        "training_epoch": 1,
        "training_frequency": 1,
        "training_iters_per_batch": 1,
        "training_min_timestep": 1000
      },
      "memory": {
        "name": "Atari",
        "batch_size": 32,
        "max_size": 250000
      },
      "net": {
        "type": "ConvNet",
        "hid_layers": [
          [
            [4, 32, [8, 8], 4, 0, [1, 1]],
            [32, 64, [4, 4], 2, 0, [1, 1]],
            [64, 64, [3, 3], 1, 0, [1, 1]]
          ],
          [512]
        ],
        "hid_layers_activation": "relu",
        "loss_spec": {
          "name": "SmoothL1Loss"
        },
        "optim_spec": {
          "name": "RMSprop",
          "lr": 0.00025,
          "alpha": 0.95,
          "eps": 0.01,
          "momentum": 0.0,
          "centered": true
        },
        "batch_norm": false,
        "lr_decay": "no_decay",
        "lr_decay_frequency": 400,
        "lr_decay_min_timestep": 1400,
        "update_type": "replace",
        "update_frequency": 10000,
        "polyak_weight": 0.9,
        "gpu": true
      }
    }],
    "env": [{
      "name": "BreakoutDeterministic-v4",
      "max_timestep": null,
      "max_episode": 50000
    }],
    "body": {
      "product": "outer",
      "num": 1
    },
    "meta": {
      "max_session": 1,
      "max_trial": 1,
      "search": "RandomSearch",
      "train_mode": true
    }
  }
}
