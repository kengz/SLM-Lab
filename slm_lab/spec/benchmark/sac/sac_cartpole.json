{
  "sac_cartpole": {
    "agent": {
      "name": "SoftActorCritic",
      "algorithm": {
        "name": "SoftActorCritic",
        "action_pdtype": "Categorical",
        "action_policy": "default",
        "gamma": 0.99,
        "target_entropy_scale": 0.584,
        "training_iter": 40,
        "training_frequency": 12,
        "training_start_step": 1000
      },
      "memory": {
        "name": "PrioritizedReplay",
        "alpha": 0.6,
        "epsilon": 0.0001,
        "batch_size": 1536,
        "max_size": 1000000,
        "use_cer": false
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [256],
        "hid_layers_activation": "tanh",
        "init_fn": "xavier_uniform_",
        "clip_grad_val": null,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "AdamW",
          "lr": 0.0003
        },
        "update_type": "polyak",
        "polyak_coef": 0.017,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "CartPole-v1",
      "num_envs": 4,
      "max_t": null,
      "max_frame": 100000
    },
    "meta": {
      "distributed": false,
      "log_frequency": 500,
      "eval_frequency": 500,
      "max_session": 1,
      "max_trial": 20,
      "search_scheduler": {
        "grace_period": 30000,
        "reduction_factor": 3
      }
    },
    "search": {
      "agent.memory.batch_size__qrandint": [1024, 4096, 1024],
      "agent.algorithm.training_iter__qrandint": [10, 30, 10],
      "agent.algorithm.training_frequency__qrandint": [10, 40, 10],
      "agent.net.polyak_coef__loguniform": [0.005, 0.5],
      "agent.net.hid_layers_activation__choice": ["tanh", "relu"]
    }
  }
}
