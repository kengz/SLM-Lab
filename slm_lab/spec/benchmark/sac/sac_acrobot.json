{
  "sac_acrobot": {
    "agent": {
      "name": "SoftActorCritic",
      "algorithm": {
        "name": "SoftActorCritic",
        "action_pdtype": "Categorical",
        "action_policy": "default",
        "gamma": 0.99,
        "alpha_lr": 0.0003,
        "training_iter": 4,
        "training_frequency": 4,
        "training_start_step": 1000
      },
      "memory": {
        "name": "Replay",
        "batch_size": 256,
        "max_size": 100000,
        "use_cer": true
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [128, 128],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "clip_grad_val": 0.5,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "AdamW",
          "lr": 0.0003
        },
        "update_type": "polyak",
        "polyak_coef": 0.005,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "Acrobot-v1",
      "num_envs": 8,
      "max_t": null,
      "max_frame": 300000
    },
    "meta": {
      "distributed": false,
      "log_frequency": 1000,
      "eval_frequency": 5000,
      "max_session": 1,
      "max_trial": 20,
      "search_scheduler": {
        "grace_period": 50000,
        "reduction_factor": 3
      }
    },
    "search": {
      "agent.algorithm.gamma__uniform": [0.98, 0.999],
      "agent.algorithm.alpha_lr__loguniform": [0.00001, 0.001],
      "agent.algorithm.training_frequency__qrandint": [1, 8, 1],
      "agent.algorithm.training_iter__qrandint": [1, 8, 1],
      "agent.memory.batch_size__choice": [128, 256, 512],
      "agent.net.hid_layers__choice": [[64, 64], [128, 128], [256, 128]],
      "agent.net.optim_spec.lr__loguniform": [0.0001, 0.001],
      "agent.net.polyak_coef__uniform": [0.001, 0.02]
    }
  }
}
