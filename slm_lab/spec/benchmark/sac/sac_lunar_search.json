{
  "sac_lunar_search_asha": {
    "agent": {
      "name": "SoftActorCritic",
      "algorithm": {
        "name": "SoftActorCritic",
        "action_pdtype": "GumbelSoftmax",
        "action_policy": "default",
        "gamma": 0.99,
        "target_entropy_epsilon": 0.1,
        "alpha_lr": 3e-05,
        "training_frequency": 1
      },
      "memory": {
        "name": "Replay",
        "batch_size": 256,
        "max_size": 100000,
        "use_cer": true
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [
          64,
          64,
          32
        ],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "clip_grad_val": 0.5,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "AdamW",
          "lr": 0.0003
        },
        "update_type": "polyak",
        "update_frequency": 1,
        "polyak_coef": 0.005,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "LunarLander-v3",
      "max_t": null,
      "max_frame": 300000,
      "num_envs": 8
    },
    "meta": {
      "distributed": false,
      "log_frequency": 1000,
      "eval_frequency": 500,
      "rigorous_eval": 0,
      "max_session": 1,
      "max_trial": 30,
      "search_scheduler": {
        "grace_period": 30000,
        "reduction_factor": 3
      }
    },
    "search": {
      "agent": {
        "algorithm": {
          "gamma__choice": [
            0.97,
            0.98,
            0.99
          ],
          "target_entropy_epsilon__uniform": [
            0.05,
            0.2
          ],
          "alpha_lr__loguniform": [
            1e-05,
            0.0001
          ]
        },
        "memory": {
          "batch_size__choice": [
            128,
            256,
            512
          ]
        },
        "net": {
          "hid_layers__choice": [
            [
              64,
              64
            ],
            [
              64,
              64,
              32
            ],
            [
              128,
              128
            ]
          ],
          "hid_layers_activation__choice": [
            "relu",
            "tanh"
          ],
          "optim_spec": {
            "lr__loguniform": [
              0.0001,
              0.001
            ]
          },
          "polyak_coef__loguniform": [
            0.001,
            0.01
          ]
        }
      }
    }
  }
}
