{
  "sac_bipedalwalker": {
    "agent": {
      "name": "SoftActorCritic",
      "algorithm": {
        "name": "SoftActorCritic",
        "action_pdtype": "default",
        "action_policy": "default",
        "gamma": 0.99,
        "training_frequency": 1,
        "training_iter": 1,
        "training_start_step": 1000
      },
      "memory": {
        "name": "Replay",
        "batch_size": 256,
        "max_size": 1000000,
        "use_cer": true
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [256, 256],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "clip_grad_val": 0.5,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "Adam",
          "lr": 0.0003
        },
        "update_type": "polyak",
        "update_frequency": 1,
        "polyak_coef": 0.005,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "BipedalWalker-v3",
      "num_envs": 16,
      "max_t": null,
      "max_frame": 5e5
    },
    "meta": {
      "distributed": false,
      "log_frequency": 10000,
      "eval_frequency": 10000,
      "max_session": 1,
      "max_trial": 8,
      "search_scheduler": {
        "grace_period": 50000,
        "reduction_factor": 3
      }
    },
    "search": {
      "agent.algorithm.gamma__uniform": [0.985, 0.999],
      "agent.algorithm.training_frequency__qrandint": [1, 4, 1],
      "agent.algorithm.training_iter__qrandint": [1, 4, 1],
      "agent.memory.batch_size__choice": [256, 512, 1024],
      "agent.net.hid_layers__choice": [[256, 256], [512, 256], [512, 512]],
      "agent.net.optim_spec.lr__loguniform": [0.0001, 0.001],
      "agent.net.polyak_coef__uniform": [0.003, 0.01]
    }
  }
}
