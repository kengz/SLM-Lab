{
  "sac_mujoco": {
    "agent": {
      "name": "SoftActorCritic",
      "algorithm": {
        "name": "SoftActorCritic",
        "action_pdtype": "default",
        "action_policy": "default",
        "gamma": 0.99,
        "training_frequency": 4,
        "training_iter": 4,
        "alpha_lr": 0.0001
      },
      "memory": {
        "name": "Replay",
        "batch_size": 256,
        "max_size": 1000000,
        "use_cer": true
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [
          256,
          256
        ],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "clip_grad_val": 1.0,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "AdamW",
          "lr": 0.0003
        },
        "update_type": "polyak",
        "update_frequency": 1,
        "polyak_coef": 0.005,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "${env}",
      "num_envs": 8,
      "max_t": null,
      "max_frame": 2e6
    },
    "meta": {
      "distributed": false,
      "log_frequency": 1000,
      "eval_frequency": 1000,
      "rigorous_eval": 0,
      "max_session": 1,
      "max_trial": 30,
      "search_scheduler": {
        "grace_period": 100000,
        "reduction_factor": 3
      }
    },
    "search": {
      "agent": {
        "algorithm": {
          "gamma__uniform": [0.98, 0.999],
          "alpha_lr__loguniform": [0.00001, 0.001],
          "training_frequency__choice": [1, 2, 4, 8],
          "training_iter__choice": [1, 2, 4, 8]
        },
        "memory": {
          "batch_size__choice": [128, 256, 512]
        },
        "net": {
          "hid_layers__choice": [
            [256, 256],
            [256, 128]
          ],
          "optim_spec": {
            "lr__loguniform": [0.0001, 0.001]
          },
          "polyak_coef__loguniform": [0.001, 0.02]
        }
      }
    }
  },
  "sac_humanoid": {
    "agent": {
      "name": "SoftActorCritic",
      "algorithm": {
        "name": "SoftActorCritic",
        "action_pdtype": "default",
        "action_policy": "default",
        "gamma": 0.99,
        "training_frequency": 1
      },
      "memory": {
        "name": "Replay",
        "batch_size": 256,
        "max_size": 1000000,
        "use_cer": true
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [
          256,
          256
        ],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "clip_grad_val": 1.0,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "AdamW",
          "lr": 0.0003
        },
        "update_type": "polyak",
        "update_frequency": 1,
        "polyak_coef": 0.005,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "Humanoid-v5",
      "num_envs": 8,
      "max_t": null,
      "max_frame": 5e6
    },
    "meta": {
      "distributed": false,
      "log_frequency": 1000,
      "eval_frequency": 1000,
      "rigorous_eval": 0,
      "max_session": 4,
      "max_trial": 1
    }
  }
}