{
  "sac_lunar": {
    "agent": {
      "name": "SoftActorCritic",
      "algorithm": {
        "name": "SoftActorCritic",
        "action_pdtype": "Categorical",
        "action_policy": "default",
        "gamma": 0.99,
        "training_frequency": 1,
        "training_iter": 1,
        "training_start_step": 5000
      },
      "memory": {
        "name": "Replay",
        "batch_size": 256,
        "max_size": 100000,
        "use_cer": false
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [256, 256],
        "hid_layers_activation": "relu",
        "init_fn": "xavier_uniform_",
        "clip_grad_val": null,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "Adam",
          "lr": 0.0003
        },
        "update_type": "polyak",
        "polyak_coef": 0.005,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "LunarLander-v3",
      "num_envs": 8,
      "max_t": null,
      "max_frame": 300000
    },
    "meta": {
      "distributed": false,
      "log_frequency": 5000,
      "eval_frequency": 5000,
      "max_session": 1,
      "max_trial": 20,
      "search_resources": {"cpu": 1, "gpu": 0.125},
      "search_scheduler": {
        "grace_period": 50000,
        "reduction_factor": 3
      }
    },
    "search": {
      "agent.algorithm.gamma__uniform": [0.98, 0.999],
      "agent.algorithm.training_frequency__qrandint": [1, 4, 1],
      "agent.algorithm.training_iter__qrandint": [1, 4, 1],
      "agent.net.optim_spec.lr__loguniform": [1e-4, 1e-3],
      "agent.net.polyak_coef__loguniform": [0.001, 0.01]
    }
  },
  "sac_lunar_continuous": {
    "agent": {
      "name": "SoftActorCritic",
      "algorithm": {
        "name": "SoftActorCritic",
        "action_pdtype": "default",
        "action_policy": "default",
        "gamma": 0.99,
        "training_iter": 1,
        "training_frequency": 1,
        "training_start_step": 5000
      },
      "memory": {
        "name": "Replay",
        "batch_size": 256,
        "max_size": 100000,
        "use_cer": false
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [256, 256],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "clip_grad_val": 0.5,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "Adam",
          "lr": 0.0003
        },
        "update_type": "polyak",
        "update_frequency": 1,
        "polyak_coef": 0.005,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "LunarLander-v3",
      "continuous": true,
      "num_envs": 8,
      "max_t": null,
      "max_frame": 300000
    },
    "meta": {
      "distributed": false,
      "log_frequency": 5000,
      "eval_frequency": 5000,
      "rigorous_eval": 0,
      "max_session": 1,
      "max_trial": 20,
      "search_resources": {"cpu": 1, "gpu": 0.125},
      "search_scheduler": {
        "grace_period": 50000,
        "reduction_factor": 3
      }
    },
    "search": {
      "agent.algorithm.gamma__uniform": [0.98, 0.999],
      "agent.algorithm.training_frequency__qrandint": [1, 4, 1],
      "agent.algorithm.training_iter__qrandint": [1, 4, 1],
      "agent.net.optim_spec.lr__loguniform": [1e-4, 1e-3],
      "agent.net.polyak_coef__loguniform": [0.001, 0.01]
    }
  }
}
