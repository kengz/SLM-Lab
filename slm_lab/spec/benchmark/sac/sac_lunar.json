{
  "sac_lunar": {
    "agent": {
      "name": "SoftActorCritic",
      "algorithm": {
        "name": "SoftActorCritic",
        "action_pdtype": "Categorical",
        "action_policy": "default",
        "gamma": 0.99,
        "alpha_lr": 3e-05,
        "training_frequency": 1
      },
      "memory": {
        "name": "Replay",
        "batch_size": 256,
        "max_size": 100000,
        "use_cer": true
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [
          64,
          64,
          32
        ],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "clip_grad_val": 0.5,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "AdamW",
          "lr": 0.0003
        },
        "update_type": "polyak",
        "update_frequency": 1,
        "polyak_coef": 0.005,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "LunarLander-v3",
      "max_t": null,
      "max_frame": 300000,
      "num_envs": 8
    },
    "meta": {
      "distributed": false,
      "log_frequency": 1000,
      "eval_frequency": 500,
      "rigorous_eval": 0,
      "max_session": 4,
      "max_trial": 1
    }
  },
  "sac_lunar_continuous": {
    "agent": {
      "name": "SoftActorCritic",
      "algorithm": {
        "name": "SoftActorCritic",
        "action_pdtype": "default",
        "action_policy": "default",
        "gamma": 0.9807,
        "training_iter": 25,
        "training_frequency": 15,
        "training_start_step": 1200
      },
      "memory": {
        "name": "Replay",
        "batch_size": 256,
        "max_size": 100000,
        "use_cer": true
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [
          256,
          256
        ],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "clip_grad_val": 0.5,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "AdamW",
          "lr": 0.0005646
        },
        "update_type": "polyak",
        "update_frequency": 1,
        "polyak_coef": 0.005949,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "LunarLander-v3",
      "continuous": true,
      "max_t": null,
      "max_frame": 300000,
      "num_envs": 8
    },
    "meta": {
      "distributed": false,
      "log_frequency": 1000,
      "eval_frequency": 500,
      "rigorous_eval": 0,
      "max_session": 4,
      "max_trial": 1
    }
  }
}
