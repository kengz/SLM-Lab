{
  "sac_lunar": {
    "agent": {
      "name": "SoftActorCritic",
      "algorithm": {
        "name": "SoftActorCritic",
        "action_pdtype": "Categorical",
        "action_policy": "default",
        "gamma": 0.99,
        "alpha_lr": 0.0003,
        "training_frequency": 1,
        "training_iter": 1,
        "training_start_step": 1000
      },
      "memory": {
        "name": "Replay",
        "batch_size": 256,
        "max_size": 100000,
        "use_cer": true
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [128, 128],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "clip_grad_val": 0.5,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "AdamW",
          "lr": 0.0003
        },
        "update_type": "polyak",
        "update_frequency": 1,
        "polyak_coef": 0.005,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "LunarLander-v3",
      "max_t": null,
      "max_frame": 500000,
      "num_envs": 8
    },
    "meta": {
      "distributed": false,
      "log_frequency": 5000,
      "eval_frequency": 5000,
      "max_session": 1,
      "max_trial": 30,
      "search_scheduler": {
        "grace_period": 80000,
        "reduction_factor": 3
      }
    },
    "search": {
      "agent.algorithm.gamma__uniform": [0.98, 0.999],
      "agent.algorithm.alpha_lr__loguniform": [0.00001, 0.001],
      "agent.algorithm.training_frequency__qrandint": [1, 8, 1],
      "agent.algorithm.training_iter__qrandint": [1, 10, 1],
      "agent.memory.batch_size__choice": [128, 256, 512],
      "agent.net.hid_layers__choice": [[64, 64], [128, 128], [256, 128], [256, 256]],
      "agent.net.optim_spec.lr__loguniform": [0.0001, 0.001],
      "agent.net.polyak_coef__uniform": [0.001, 0.02]
    }
  },
  "sac_lunar_continuous": {
    "agent": {
      "name": "SoftActorCritic",
      "algorithm": {
        "name": "SoftActorCritic",
        "action_pdtype": "default",
        "action_policy": "default",
        "gamma": 0.9807,
        "training_iter": 25,
        "training_frequency": 15,
        "training_start_step": 1200
      },
      "memory": {
        "name": "Replay",
        "batch_size": 256,
        "max_size": 100000,
        "use_cer": true
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [
          256,
          256
        ],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "clip_grad_val": 0.5,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "AdamW",
          "lr": 0.0005646
        },
        "update_type": "polyak",
        "update_frequency": 1,
        "polyak_coef": 0.005949,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "LunarLander-v3",
      "continuous": true,
      "max_t": null,
      "max_frame": 300000,
      "num_envs": 8
    },
    "meta": {
      "distributed": false,
      "log_frequency": 1000,
      "eval_frequency": 500,
      "rigorous_eval": 0,
      "max_session": 4,
      "max_trial": 1
    }
  }
}
