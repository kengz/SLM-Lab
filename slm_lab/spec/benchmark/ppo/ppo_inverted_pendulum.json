{
  "ppo_inverted_pendulum": {
    "agent": {
      "name": "PPO",
      "algorithm": {
        "name": "PPO",
        "action_pdtype": "default",
        "action_policy": "default",
        "explore_var_spec": null,
        "gamma": 0.999,
        "lam": 0.9,
        "clip_eps_spec": {
          "name": "no_decay",
          "start_val": 0.4,
          "end_val": 0.4,
          "start_step": 0,
          "end_step": 0
        },
        "entropy_coef_spec": {
          "name": "no_decay",
          "start_val": 0.0,
          "end_val": 0.0,
          "start_step": 0,
          "end_step": 0
        },
        "val_loss_coef": 0.2,
        "time_horizon": 512,
        "minibatch_size": 64,
        "training_epoch": 5,
        "normalize_v_targets": true
      },
      "memory": {
        "name": "OnPolicyBatchReplay"
      },
      "net": {
        "type": "MLPNet",
        "shared": false,
        "hid_layers": [64, 64],
        "hid_layers_activation": "tanh",
        "init_fn": "orthogonal_",
        "normalize": true,
        "clip_grad_val": 0.3,
        "use_same_optim": true,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "Adam",
          "lr": 0.000222
        },
        "gpu": "auto"
      }
    },
    "env": {
      "name": "InvertedPendulum-v5",
      "num_envs": 16,
      "max_t": null,
      "max_frame": 1000000
    },
    "meta": {
      "distributed": false,
      "log_frequency": 10000,
      "eval_frequency": 10000,
      "rigorous_eval": 0,
      "max_session": 1,
      "max_trial": 8,
      "search_scheduler": {
        "grace_period": 100000,
        "reduction_factor": 3
      }
    },
    "search": {
      "agent.algorithm.gamma__uniform": [0.995, 0.9999],
      "agent.algorithm.lam__uniform": [0.85, 0.95],
      "agent.algorithm.clip_eps_spec.start_val__uniform": [0.3, 0.5],
      "agent.net.optim_spec.lr__loguniform": [0.0001, 0.0005]
    }
  }
}
