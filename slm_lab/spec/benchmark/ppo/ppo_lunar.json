{
  "ppo_lunar": {
    "agent": {
      "name": "PPO",
      "algorithm": {
        "name": "PPO",
        "action_pdtype": "default",
        "action_policy": "default",
        "explore_var_spec": null,
        "gamma": 0.99,
        "lam": 0.95,
        "clip_eps_spec": {
          "name": "no_decay",
          "start_val": 0.2,
          "end_val": 0.0,
          "start_step": 10000,
          "end_step": 300000
        },
        "entropy_coef_spec": {
          "name": "no_decay",
          "start_val": 0.01,
          "end_val": 0.01,
          "start_step": 0,
          "end_step": 0
        },
        "val_loss_coef": 1.0,
        "time_horizon": 128,
        "minibatch_size": 256,
        "training_epoch": 10
      },
      "memory": {
        "name": "OnPolicyBatchReplay"
      },
      "net": {
        "type": "MLPNet",
        "shared": false,
        "hid_layers": [
          64,
          64,
          32
        ],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "batch_norm": false,
        "clip_grad_val": 0.5,
        "use_same_optim": true,
        "loss_spec": {
          "name": "MSELoss"
        },
        "actor_optim_spec": {
          "name": "AdamW",
          "lr": 0.0005
        },
        "critic_optim_spec": {
          "name": "AdamW",
          "lr": 0.0005
        },
        "gpu": "auto"
      }
    },
    "env": {
      "name": "LunarLander-v3",
      "max_t": null,
      "max_frame": 300000,
      "num_envs": 8
    },
    "meta": {
      "distributed": false,
      "log_frequency": 1000,
      "eval_frequency": 1000,
      "max_session": 1,
      "max_trial": 30,
      "search_scheduler": {
        "grace_period": 30000,
        "reduction_factor": 3
      }
    },
    "search": {
      "agent": {
        "algorithm": {
          "gamma__uniform": [
            0.95,
            0.999
          ],
          "lam__uniform": [
            0.85,
            0.99
          ],
          "clip_eps_spec": {
            "start_val__uniform": [
              0.05,
              0.4
            ]
          },
          "entropy_coef_spec": {
            "start_val__loguniform": [
              1e-5,
              0.03
            ]
          },
          "val_loss_coef__loguniform": [
            0.2,
            3.0
          ],
          "time_horizon__choice": [
            64,
            128,
            256
          ],
          "minibatch_size__choice": [
            128,
            256,
            512
          ],
          "training_epoch__randint": [
            4,
            16
          ]
        },
        "net": {
          "shared__choice": [
            true,
            false
          ],
          "hid_layers__choice": [
            [
              64,
              64
            ],
            [
              128,
              64
            ],
            [
              64,
              64,
              32
            ],
            [
              128,
              128,
              64
            ]
          ],
          "hid_layers_activation__choice": [
            "relu",
            "tanh"
          ],
          "init_fn__choice": [
            "orthogonal_",
            "xavier_uniform_"
          ],
          "actor_optim_spec": {
            "lr__loguniform": [
              1e-5,
              5e-3
            ]
          },
          "critic_optim_spec": {
            "lr__loguniform": [
              1e-5,
              5e-3
            ]
          }
        }
      }
    }
  }
}
