{
  "ppo_lunar": {
    "agent": {
      "name": "PPO",
      "algorithm": {
        "name": "PPO",
        "action_pdtype": "default",
        "action_policy": "default",
        "explore_var_spec": null,
        "gamma": 0.9931,
        "lam": 0.9675,
        "clip_eps_spec": {
          "name": "linear_decay",
          "start_val": 0.1521,
          "end_val": 0.2373,
          "start_step": 0,
          "end_step": 2000000
        },
        "entropy_coef_spec": {
          "name": "linear_decay",
          "start_val": 0.0311,
          "end_val": 0.0000411,
          "start_step": 0,
          "end_step": 2000000
        },
        "val_loss_coef": 0.8417,
        "time_horizon": 256,
        "minibatch_size": 256,
        "training_epoch": 15
      },
      "memory": {
        "name": "OnPolicyBatchReplay"
      },
      "net": {
        "type": "MLPNet",
        "shared": false,
        "hid_layers": [256, 256, 128],
        "hid_layers_activation": "relu",
        "init_fn": "orthogonal_",
        "batch_norm": false,
        "clip_grad_val": 0.5,
        "use_same_optim": false,
        "loss_spec": {
          "name": "MSELoss"
        },
        "actor_optim_spec": {
          "name": "Adam",
          "lr": 0.000163
        },
        "critic_optim_spec": {
          "name": "Adam",
          "lr": 0.000268
        },
        "gpu": "auto"
      }
    },
    "env": {
      "name": "LunarLander-v3",
      "max_t": null,
      "max_frame": 2000000,
      "num_envs": 8
    },
    "meta": {
      "distributed": false,
      "log_frequency": 1000,
      "eval_frequency": 1000,
      "max_session": 4,
      "max_trial": 1
    }
  }
}
