{
  "ppo_halfcheetah": {
    "agent": {
      "name": "PPO",
      "algorithm": {
        "name": "PPO",
        "action_pdtype": "default",
        "action_policy": "default",
        "explore_var_spec": null,
        "gamma": 0.98,
        "lam": 0.92,
        "clip_eps_spec": {
          "name": "no_decay",
          "start_val": 0.1,
          "end_val": 0.1,
          "start_step": 0,
          "end_step": 0
        },
        "entropy_coef_spec": {
          "name": "no_decay",
          "start_val": 0.0004,
          "end_val": 0.0004,
          "start_step": 0,
          "end_step": 0
        },
        "val_loss_coef": 0.58,
        "time_horizon": 512,
        "minibatch_size": 64,
        "training_epoch": 20,
        "normalize_v_targets": true
      },
      "memory": {
        "name": "OnPolicyBatchReplay"
      },
      "net": {
        "type": "MLPNet",
        "shared": false,
        "hid_layers": [64, 64],
        "hid_layers_activation": "tanh",
        "init_fn": "orthogonal_",
        "normalize": true,
        "clip_grad_val": 0.8,
        "use_same_optim": true,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "Adam",
          "lr": 2.06e-05
        },
        "gpu": "auto"
      }
    },
    "env": {
      "name": "HalfCheetah-v5",
      "num_envs": 16,
      "max_t": null,
      "max_frame": 3000000
    },
    "meta": {
      "distributed": false,
      "log_frequency": 10000,
      "eval_frequency": 10000,
      "rigorous_eval": 0,
      "max_session": 1,
      "max_trial": 12,
      "search_resources": {"cpu": 1, "gpu": 0.125},
      "search_scheduler": {
        "grace_period": 300000,
        "reduction_factor": 3
      }
    },
    "search": {
      "agent.algorithm.gamma__uniform": [0.97, 0.99],
      "agent.algorithm.lam__uniform": [0.90, 0.95],
      "agent.algorithm.clip_eps_spec.start_val__uniform": [0.08, 0.15],
      "agent.net.optim_spec.lr__loguniform": [1e-05, 5e-05]
    }
  }
}
