{
  "ppo_cartpole_search1": {
    "agent": {
      "name": "PPO",
      "algorithm": {
        "name": "PPO",
        "action_pdtype": "default",
        "action_policy": "default",
        "explore_var_spec": null,
        "gamma": 0.99,
        "lam": 0.95,
        "clip_eps_spec": {
          "name": "no_decay",
          "start_val": 0.2,
          "end_val": 0.2,
          "start_step": 0,
          "end_step": 200000
        },
        "entropy_coef_spec": {
          "name": "no_decay",
          "start_val": 0.01,
          "end_val": 0.01,
          "start_step": 0,
          "end_step": 200000
        },
        "val_loss_coef": 0.5,
        "time_horizon": 128,
        "minibatch_size": 64,
        "training_epoch": 4
      },
      "memory": {
        "name": "OnPolicyBatchReplay"
      },
      "net": {
        "type": "MLPNet",
        "shared": true,
        "hid_layers": [64],
        "hid_layers_activation": "relu",
        "clip_grad_val": 0.5,
        "use_same_optim": false,
        "loss_spec": {
          "name": "SmoothL1Loss"
        },
        "actor_optim_spec": {
          "name": "AdamW",
          "lr": 0.001
        },
        "critic_optim_spec": {
          "name": "AdamW",
          "lr": 0.001
        },
        "gpu": "auto"
      }
    },
    "env": {
      "name": "CartPole-v1",
      "max_t": null,
      "max_frame": 200000,
      "num_envs": 4
    },
    "meta": {
      "distributed": false,
      "log_frequency": 2000,
      "eval_frequency": 2000,
      "max_session": 1,
      "max_trial": 200,
      "search_scheduler": {
        "grace_period": 20000,
        "reduction_factor": 3
      }
    },
    "search": {
      "agent.algorithm.gamma__uniform": [0.95, 0.999],
      "agent.algorithm.lam__uniform": [0.9, 0.99],
      "agent.algorithm.clip_eps_spec.start_val__uniform": [0.1, 0.3],
      "agent.algorithm.entropy_coef_spec.start_val__loguniform": [0.0001, 0.1],
      "agent.algorithm.val_loss_coef__uniform": [0.1, 1.0],
      "agent.algorithm.time_horizon__choice": [64, 128, 256, 512],
      "agent.algorithm.minibatch_size__choice": [32, 64, 128],
      "agent.algorithm.training_epoch__choice": [3, 4, 5, 8, 10],
      "agent.net.loss_spec.name__choice": ["MSELoss", "SmoothL1Loss", "L1Loss"],
      "agent.net.actor_optim_spec.lr__loguniform": [0.0001, 0.01],
      "agent.net.critic_optim_spec.lr__loguniform": [0.0001, 0.01]
    }
  }
}
