{
  "ppo_shared_cartpole": {
    "agent": {
      "name": "PPO",
      "algorithm": {
        "name": "PPO",
        "action_pdtype": "default",
        "action_policy": "default",
        "explore_var_spec": null,
        "gamma": 0.99,
        "lam": 0.95,
        "clip_eps_spec": {
          "name": "no_decay",
          "start_val": 0.2,
          "end_val": 0.05,
          "start_step": 0,
          "end_step": 10000
        },
        "entropy_coef_spec": {
          "name": "linear_decay",
          "start_val": 0.01,
          "end_val": 0.001,
          "start_step": 1000,
          "end_step": 5000
        },
        "val_loss_coef": 0.1,
        "time_horizon": 32,
        "minibatch_size": 16,
        "training_epoch": 4
      },
      "memory": {
        "name": "OnPolicyBatchReplay"
      },
      "net": {
        "type": "MLPNet",
        "shared": true,
        "hid_layers": [
          128
        ],
        "hid_layers_activation": "relu",
        "clip_grad_val": 0.5,
        "use_same_optim": false,
        "loss_spec": {
          "name": "MSELoss"
        },
        "actor_optim_spec": {
          "name": "AdamW",
          "lr": 0.02
        },
        "critic_optim_spec": {
          "name": "AdamW",
          "lr": 0.02
        },
        "gpu": "auto"
      }
    },
    "env": {
      "name": "CartPole-v1",
      "max_t": null,
      "max_frame": 20000
    },
    "meta": {
      "distributed": false,
      "log_frequency": 500,
      "eval_frequency": 100,
      "max_session": 1,
      "max_trial": 3,
      "search_scheduler": {
        "grace_period": 3000,
        "reduction_factor": 2
      }
    },
    "search": {
      "agent": {
        "algorithm": {
          "gamma__uniform": [
            0.9,
            0.999
          ],
          "lam__uniform": [
            0.7,
            0.98
          ],
          "clip_eps_spec": {
            "start_val__uniform": [
              0.05,
              0.4
            ]
          },
          "entropy_coef_spec": {
            "start_val__loguniform": [
              1e-4,
              0.1
            ]
          },
          "val_loss_coef__loguniform": [
            0.01,
            1.0
          ],
          "time_horizon__choice": [
            128,
            256
          ],
          "training_epoch__choice": [
            4,
            6,
            8
          ]
        },
        "net": {
          "actor_optim_spec": {
            "lr__loguniform": [
              1e-4,
              0.05
            ]
          },
          "critic_optim_spec": {
            "lr__loguniform": [
              1e-4,
              0.05
            ]
          }
        }
      }
    }
  }
}
