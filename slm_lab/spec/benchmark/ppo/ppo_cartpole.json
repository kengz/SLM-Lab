{
  "ppo_shared_cartpole": {
    "agent": {
      "name": "PPO",
      "algorithm": {
        "name": "PPO",
        "action_pdtype": "default",
        "action_policy": "default",
        "explore_var_spec": null,
        "gamma": 0.99,
        "lam": 0.95,
        "clip_eps_spec": {
          "name": "linear_decay",
          "start_val": 0.1702,
          "end_val": 0.1339,
          "start_step": 0,
          "end_step": 500000
        },
        "entropy_coef_spec": {
          "name": "linear_decay",
          "start_val": 0.00107,
          "end_val": 0.00143,
          "start_step": 0,
          "end_step": 500000
        },
        "val_loss_coef": 0.2488,
        "time_horizon": 64,
