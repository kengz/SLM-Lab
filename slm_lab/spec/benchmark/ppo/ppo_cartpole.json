        "critic_optim_spec": {
          "name": "AdamW",
          "lr": 0.02
        },
        "gpu": "auto"
      }
    },
    "env": {
      "name": "CartPole-v1",
      "max_t": null,
      "max_frame": 200000,
      "num_envs": 4
    },
    "meta": {
      "distributed": false,
      "log_frequency": 1000,
      "eval_frequency": 1000,
      "max_session": 1,
      "max_trial": 1
    }
  }
}
