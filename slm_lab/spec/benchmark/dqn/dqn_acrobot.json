{
  "dqn_epsilon_greedy_acrobot": {
    "agent": {
      "name": "DQN",
      "algorithm": {
        "name": "DQN",
        "action_pdtype": "Argmax",
        "action_policy": "epsilon_greedy",
        "explore_var_spec": {
          "name": "linear_decay",
          "start_val": 1.0,
          "end_val": 0.1,
          "start_step": 0,
          "end_step": 10000
        },
        "gamma": 0.99,
        "training_batch_iter": 2,
        "training_iter": 2,
        "training_frequency": 4,
        "training_start_step": 32
      },
      "memory": {
        "name": "Replay",
        "batch_size": 32,
        "max_size": 50000,
        "use_cer": true
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [
          128,
          128
        ],
        "hid_layers_activation": "relu",
        "clip_grad_val": 1.0,
        "loss_spec": {
          "name": "SmoothL1Loss"
        },
        "optim_spec": {
          "name": "Adam",
          "lr": 0.0005
        },
        "update_type": "replace",
        "update_frequency": 100,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "Acrobot-v1",
      "max_t": null,
      "max_frame": 300000
    },
    "meta": {
      "distributed": false,
      "eval_frequency": 500,
      "max_session": 4,
      "max_trial": 1,
      "log_frequency": 500
    }
  },
  "dqn_boltzmann_acrobot": {
    "agent": {
      "name": "DQN",
      "algorithm": {
        "name": "DQN",
        "action_pdtype": "Categorical",
        "action_policy": "boltzmann",
        "explore_var_spec": {
          "name": "linear_decay",
          "start_val": 1.5,
          "end_val": 0.055,
          "start_step": 0,
          "end_step": 10000
        },
        "gamma": 0.997,
        "training_batch_iter": 2,
        "training_iter": 2,
        "training_frequency": 4,
        "training_start_step": 32
      },
      "memory": {
        "name": "Replay",
        "batch_size": 64,
        "max_size": 50000,
        "use_cer": true
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [
          256,
          128
        ],
        "hid_layers_activation": "selu",
        "clip_grad_val": 0.54,
        "loss_spec": {
          "name": "SmoothL1Loss"
        },
        "optim_spec": {
          "name": "Adam",
          "lr": 0.00031
        },
        "update_type": "replace",
        "update_frequency": 50,
        "gpu": "auto"
      }
    },
    "env": {
      "name": "Acrobot-v1",
      "max_t": null,
      "max_frame": 300000
    },
    "meta": {
      "distributed": false,
      "eval_frequency": 500,
      "max_session": 4,
      "max_trial": 1
    }
  }
}
