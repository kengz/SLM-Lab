{
  "unit_test_dqn": {
    "agent": [{
      "name": "DQN",
      "algorithm": {
        "name": "DQN",
        "action_policy": "boltzmann",
        "action_policy_update": "linear_decay",
        "explore_var_start": 1.5,
        "explore_var_end": 0.3,
        "explore_anneal_epi": 10,
        "gamma": 0.999,
        "training_min_timestep": 10,
        "training_frequency": 1,
        "training_epoch": 5,
        "training_iters_per_batch": 3
      },
      "memory": {
        "name": "Replay",
        "max_size": 1000
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [64],
        "hid_layers_activation": "relu",
        "optim": {
          "name": "Adam",
          "lr": 0.02
        },
        "loss": {
          "name": "mse_loss"
        },
        "batch_size": 16,
        "update_type": "replace",
        "update_frequency": 1.0,
        "polyak_weight": 0,
        "decay_lr": false,
        "decay_lr_min_timestep": 1200,
        "decay_lr_frequency": 400,
      }
    }],
    "env": [{
      "name": "CartPole-v0",
      "max_timestep": null
    }],
    "body": {
      "product": "outer",
      "num": 1
    },
    "meta": {
      "max_episode": 12,
      "max_session": 1,
      "max_trial": 1,
      "train_mode": true
    }
  }
}
