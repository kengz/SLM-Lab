{
  "vanilla_dqn_epsilon_greedy_lunar": {
    "agent": [{
      "name": "VanillaDQN",
      "algorithm": {
        "name": "VanillaDQN",
        "action_pdtype": "Argmax",
        "action_policy": "epsilon_greedy",
        "action_policy_update": "linear_decay",
        "explore_var_start": 1.0,
        "explore_var_end": 0.08,
        "explore_anneal_epi": 100,
        "gamma": 0.99,
        "training_batch_epoch": 3,
        "training_epoch": 4,
        "training_frequency": 4,
        "training_min_timestep": 32,
        "normalize_state": true
      },
      "memory": {
        "name": "Replay",
        "batch_size": 32,
        "max_size": 100000,
        "use_cer": false,
        "concat_len": 4,
      },
      "net": {
        "type": "MLPNet",
        "hid_layers": [400, 200],
        "hid_layers_activation": "relu",
        "clip_grad": false,
        "clip_grad_val": 1.0,
        "loss_spec": {
          "name": "MSELoss"
        },
        "optim_spec": {
          "name": "Adam",
          "lr": 0.02
        },
        "lr_decay": "linear_decay",
        "lr_decay_frequency": 80000,
        "lr_decay_min_timestep": 40000,
        "lr_anneal_timestep": 400000,
        "update_type": "polyak",
        "update_frequency": 200,
        "polyak_coef": 0,
        "gpu": true
      }
    }],
    "env": [{
      "name": "LunarLander-v2",
      "max_timestep": null,
      "max_episode": 800,
      "save_epi_frequency": 800
    }],
    "body": {
